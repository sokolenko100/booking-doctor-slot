{
  "name": "DoctorApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "lint:fix": "eslint src/ --fix",
    "husky": "npm run fix:eslint && npm run fix:prettier && npm run test",
    "fix": "npm run fix:eslint && npm run fix:prettier",
    "fix:prettier": "prettier --write './src/**/*.{ts,tsx}'",
    "fix:eslint": "eslint --fix './src/**/*.{ts,tsx}'",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint  './src/**/*.{ts,tsx}'",
    "lint:prettier": "prettier --check './src/**/*.{ts,tsx}'",
    "clean:android": "cd android && ./gradlew clean && cd ../",
    "clean:ios": "cd ios && xcodebuild clean && cd ../",
    "clean-build": "\\rm -fr ./node_modules && \\rm -fr dist/* && \\rm -fr ios/build ios/Pods ios/KScoreApp.xcarchive && \\rm -fr android/build android/app/build && npm run full-install",
    "clean-cache": "watchman watch-del-all && rm -rf $TMPDIR/react-native-packager-cache-* &&rm -rf $TMPDIR/metro-bundler-cache-* && rm -rf node_modules/ && npm cache clean --force && npm install && npm run clean-metro-cache",
    "clean-metro-cache": "react-native start -- --reset-cache",
    "full-reinstall": "npm run clean-cache &&  npm run full-install",
    "full-install": "npm i && cd ios && pod install && cd ..",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/stack": "^6.4.1",
    "axios": "^1.7.2",
    "moment": "^2.30.1",
    "react": "18.2.0",
    "react-native": "0.74.3",
    "react-native-gesture-handler": "^2.17.1",
    "react-native-reanimated": "^3.14.0",
    "react-native-safe-area-context": "^4.10.8",
    "react-native-screens": "^3.32.0",
    "react-native-vector-icons": "^10.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.74.85",
    "@react-native/eslint-config": "0.74.85",
    "@react-native/metro-config": "0.74.85",
    "@react-native/typescript-config": "0.74.85",
    "@testing-library/react-native": "^12.5.1",
    "@tsconfig/react-native": "^3.0.5",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.3",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^18.3.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
